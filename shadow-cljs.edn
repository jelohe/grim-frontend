{:source-paths
 ["src/main"
  "src/test"]

 :dependencies
 [[reagent            "1.0.0"]
  [binaryage/devtools "1.0.2"]]

 :dev-http
 {3000 "public"}

 :builds
 {:app
  {:target     :browser
   :output-dir "public/js"
   :asset-path "/js"
   :modules    {:main
                {:init-fn grimorio/init!}}}

  :test
  {:target     :node-test
   :output-to  "out/tests.js"
   :ns-regexp  "-test$"
   :autorun    true}}}
